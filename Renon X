local Section = Tab:CreateSection("Mega Marble Run Pit Script (if you got callback error pls join Mega Marble Run Pit)")
local Button = Tab:CreateButton({
   Name = "Start auto farm Credits",
   Callback = function()
   _G.Running = true
while _G.Running == true do
firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"], game:GetService("Workspace").Movers.Begining["Touch Me To Go to Spawn Area"].Head, 0)
wait()
firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"], game:GetService("Workspace").Movers.Begining["Touch Me To Go to Spawn Area"].Head, 1)
end
   end,
})
local Button = Tab:CreateButton({
   Name = "Stop auto farm Credits",
   Callback = function()
   _G.Running = false
while _G.Running == true do
firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"], game:GetService("Workspace").Movers.Begining["Touch Me To Go to Spawn Area"].Head, 0)
wait()
firetouchinterest(game.Players.LocalPlayer.Character["Left Leg"], game:GetService("Workspace").Movers.Begining["Touch Me To Go to Spawn Area"].Head, 1)
end
   end,
})

